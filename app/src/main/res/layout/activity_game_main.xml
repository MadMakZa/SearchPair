<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fromYDelta="100%p" android:toYDelta="0%p"
    android:duration="@android:integer/config_longAnimTime"
    android:background="@drawable/background_pyramides"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/idImageLogo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/logo_name_game" />

    <Button
        android:id="@+id/btn_new_game"
        android:layout_marginTop="80dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|center"
        android:fontFamily="@font/caesar_dressing"
        android:text="@string/button_new_game"
        android:textColor="@color/colorRedDark"
        android:textSize="40sp"
        android:textStyle="bold"
        android:background="@drawable/button_new_game_theme" />

    <Button
        android:id="@+id/button_open_chest"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_gravity="top|center"
        android:layout_marginTop="130dp"
        android:background="@drawable/button_chest_closed"/>


    <ImageView
        android:id="@+id/id_text_select_level"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="110dp"
        android:layout_gravity="center"
        android:background="@drawable/select_level_text"/>

